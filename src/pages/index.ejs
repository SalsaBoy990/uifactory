    </li>
  </ul>
</div>
<div class="tab-content" id="pills-tabContent">
  <div class="tab-pane fade show active" id="pills-all" role="tabpanel" aria-labelledby="pills-all-tab">
    <% for (var i = 0, j = 0; i < postsDataForIndexPage.length; i++) { %>
    <% if (postsDataForIndexPage[i].type !== 'blog') { %>
    <div class="row m0 p0">
      <div class="col-xs-12 col-sm-12 col-md-12 col-lg-10 col-xl-10 mt0 mb2 p0 m0">
        <article>
          <div class="bg-light-gray border-shadow border-gray-4 plhalf prhalf pbhalf mb1">
            <h3 class="h3 mb1 mthalf"><a href="<%= postsDataForIndexPage[i].pathToPost %>">
                <%= postsDataForIndexPage[i].title %></a>
              <% if (postsDataForIndexPage[i].type === 'website') { %>
              <span class="badge badge-secondary white min-size normal">honlap</span>
              <% } else if (postsDataForIndexPage[i].type === 'webapp') { %>
              <span class="badge badge-secondary white min-size normal">webapp</span>
              <% } else if (postsDataForIndexPage[i].type === 'dashboard') { %>
              <span class="badge badge-secondary white min-size normal">vezérlőpult</span>
              <% } else if (postsDataForIndexPage[i].type === 'bc') { %>
              <span class="badge badge-secondary white min-size normal">névjegykártya</span>
              <% } else if (postsDataForIndexPage[i].type === 'drawing') { %>
              <span class="badge badge-secondary white min-size normal">rajz</span>
              <% } else { %>
              <span class="badge badge-secondary white min-size normal">
                <%= postsDataForIndexPage[i].type %></span>
              <% } %>
            </h3>
            <!--<p class="mbhalf">
              <%= postsDataForIndexPage[i].excerpt %>
            </p>-->
            <a href="<%= postsDataForIndexPage[i].pathToPost %>">
              <img class="pl0 m0 thumbnail-img" src="/assets/images/design/<%= postsDataForIndexPage[i].thumbnail %>"
                alt="<%= postsDataForIndexPage[i].title %>">
            </a>
          </div>
        </article>
      </div>
    </div>
    <% } %>
    <% } %>
  </div>

  <div class="tab-pane fade" id="pills-website" role="tabpanel" aria-labelledby="pills-website-tab">
    <% for (var i = 0, count = 0; i < postsDataForIndexPage.length; i++) { %>
    <% if (postsDataForIndexPage[i].type === 'website') { %>
    <div class="row m0 p0">
      <div class="col-xs-12 col-sm-12 col-md-12 col-lg-10 col-xl-10 mt0 mb2 p0 m0">
        <article>
          <div class="bg-light-gray border-shadow border-gray-4 plhalf prhalf pbhalf mb1">
            <h3 class="h3 mb1 mthalf"><a href="<%= postsDataForIndexPage[i].pathToPost %>">
                <%= postsDataForIndexPage[i].title %></a>
              <span class="badge badge-secondary white min-size normal">honlap</span>
            </h3>
            <!--<p class="mbhalf">
              <%= postsDataForIndexPage[i].excerpt %>
            </p>-->
            <a href="<%= postsDataForIndexPage[i].pathToPost %>">
              <img class="pl0 m0 thumbnail-img" src="/assets/images/design/<%= postsDataForIndexPage[i].thumbnail %>"
                alt="<%= postsDataForIndexPage[i].title %>">
            </a>
          </div>
        </article>
      </div>
    </div>
    <% count = count + 1; %>
    <% } %>
    <% } %>
    <% if (count === 0) { %>
    <p class="mb2 m-mxhalf">Még nincs feltöltve design ebből a kategóriából. :(</p>
    <% } %>
  </div>

